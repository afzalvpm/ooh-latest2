<!doctype html>
<html class="no-js">
<head>
	<meta charset="utf-8">
	<title>Job Progress</title>
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<meta name="description" content="">
	<meta name="viewport" content="width=device-width,initial-scale=1">
	<!-- Favicons -->
	<link rel="shortcut icon" href="images/logo.png">
	
	<!-- CSS STYLES -->
	<link rel="stylesheet" type="text/css" href="css/bootstrap.min.css">
	<link rel="stylesheet" type="text/css" href="css/style.css">
	<link rel="stylesheet" type="text/css" href="css/header.css">
	<link rel="stylesheet" type="text/css" href="css/font.css">
	<link rel="stylesheet" type="text/css" href="css/sidebar.css">
	<link rel="stylesheet" type="text/css" href="css/notification.css">
	<link rel="stylesheet" type="text/css" href="css/inspection.css">
	<link rel="stylesheet" type="text/css" href="css/button.css">
	<link rel="stylesheet" type="text/css" href="css/signin.css">
	<link rel="stylesheet" type="text/css" href="css/jobs.css">
	<link rel="stylesheet" type="text/css" href="css/font-awesome.css">
	<script src='https://api.mapbox.com/mapbox.js/v3.0.1/mapbox.js'></script>
	<link href='https://api.mapbox.com/mapbox.js/v3.0.1/mapbox.css' rel='stylesheet' />
	<link rel="stylesheet" type="text/css" href="http://www.jqueryscript.net/demo/Lightweight-Interactive-Pagination-Plugin-jQuery-jqPagination/css/jqpagination.css">
</head>
<body>
	<div class="overflow-scroll">
		<!-- Background overlay -->
		<div class="black-opc hide"></div>

		<!-- Fixed header -->
		<section class="navbar navbar-default navbar-fixed-top" id="header">
			<div class="container-fluid">
				<div class="navbar-header">
					<button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
						<span class="sr-only">Toggle navigation</span>
						<span class="icon-bar"></span>
						<span class="icon-bar"></span>
						<span class="icon-bar"></span>
					</button>
					<label class="hamburger-menu">
						<img src="images/hamburger.png">
					</label>
					<a class="navbar-brand" href="#">
						<span class="enterprise-logo">
							<img src="images/logo.png">
						</span>
						<!--<span class="enterprise-name">
							Outdoor inspection Services Group
							<span class="enterprise-tagline">
							Outdoor inspection Services Group
							</span>
						</span>-->
					</a>
				</div>
				<div id="navbar" class="navbar-collapse collapse">
					<ul class="nav navbar-nav navbar-right">
						<li>
							<div class="dropdown">
								<div class="profile-dropdown" data-toggle="dropdown">
									<div class="profile-desc">
										<h5>Admin</h5>
									</div>
									<img src="images/dropdown.png" class="dropdown-img">
								</div>
								<div class="notification-popup header-notification profile-popup dropdown-menu">
									<div class="notification-area">
										<div class="n-desc">
											<div class="a-profile">
												<a href="">Sign Out</a>
											</div>
										</div>
									</div>
								</div>
							</div>
						</li>
					</ul>
				</div>
			</div>
		</section>
		
		<!-- main wrapper -->
		<section id="main-wrapper">
			<!-- sidebar -->
			<div id="sidebar">
				<div class="sidebar-list text-left">
					<ul class="list-unstyled">
						<li>
							<a href="/inspection-landing">
								<img src="images/sidebar/inspection.png" alt="home">
								<label>inspection</label>
							</a>
						</li>
						<li class="active">
							<a href="/jobs-landing">
								<img src="images/sidebar/jobs.png" alt="chat">
								<label>jobs</label>
							</a>
						</li>
						<li>
							<a href="/maps-view">
								<img src="images/sidebar/maps.png" alt="contact">
								<label>Maps View</label>
							</a>
						</li>
						<li>
							<a href="/user-landing">
								<img src="images/sidebar/user.png" alt="profile">
								<label>Users</label>
							</a>
						</li>
					</ul>
				</div>
			</div>
			<!-- main wrapper -->
			<div class="container-fluid" id="wrapper-container">
				<div class="col-md-12 inspection-list jobs-tab">
					<div class="col-md-3 mpad0 text-center">
						<div class="inspection-data">
							<a href="/jobs-landing">
								<img src="images/job1.png" alt="">
								<label>View Active Jobs</label>
							</div>
						</a>
					</div>
					<div class="col-md-3 mpad0 text-center">
						<div class="inspection-data">
							<a href="/jobs-confirmed">
								<img src="images/job2.png" alt="">
								<label>View Completed Jobs</label>
							</a>
						</div>
					</div>
					<div class="col-md-3 mpad0 text-center">
						<div class="inspection-data">
							<a href="/jobs-progress">
								<img src="images/job3.png" alt="">
								<label>Jobs in Progress</label>
							</a>
						</div>
					</div>
					<div class="col-md-3 mpad0 text-center">
						<div class="inspection-data active">
							<a href="/jobs-assigned">
								<img src="images/job3.png" alt="">
								<label>View Assigned Jobs</label>
							</a>
						</div>
					</div>
				</div>
			</div>
			<div class="col-md-12" id="confirm-section">
				<div class="booking-details">
					<div class="b-header">
						<h5>Jobs In Progress</h5>
					</div>
					<div class="table-responsive">
						<table class="table">
							<thead>
								<tr>
									<th data-sort="string" data-type="name">siteId <i class="fa fa-sort" aria-hidden="true"></i> </th>
									<th data-sort="string" data-type="status">Location <i class="fa fa-sort" aria-hidden="true"></i> </th>
									<th data-sort="string">GeoCodedLocation <i class="fa fa-sort" aria-hidden="true"></th>
									<th data-sort="string">State <i class="fa fa-sort" aria-hidden="true"></th>
									<th data-sort="string">Postal Code <i class="fa fa-sort" aria-hidden="true"></th>
									<th>Location on MAP</th>
									<th data-sort="string">Start Date <i class="fa fa-sort" aria-hidden="true"></i></th>
									<th data-sort="string">End Date <i class="fa fa-sort" aria-hidden="true"></i></th>
								</tr>
							</thead>
							<tbody id="job-list">
							</tbody>
						</table>
					</div>
					<nav class="text-center">
						<div class="limitData pull-right">
							<label for="pagination-limit">Pagination Limit</label>
							<select class="form-control" id="pagination-limit">
								<option value="10">10</option>
								<option value="20">20</option>
								<option value="30">30</option>
								<option value="40">40</option>
								<option value="50">50</option>
								<option value="60">60</option>
								<option value="70">70</option>
								<option value="80">80</option>
								<option value="90">90</option>
								<option value="100">100</option>
								<option value="200">200</option>
								<option value="300">300</option>
								<option value="400">400</option>
								<option value="500">500</option>
							</select>
						</div>
						<div class="pagination">
							<a href="#" class="first" data-action="first">&laquo;</a>
							<a href="#" class="previous" data-action="previous">&lsaquo;</a>
							<input type="text" readonly="readonly" data-max-page="10" />
							<a href="#" class="next" data-action="next">&rsaquo;</a>
							<a href="#" class="last" data-action="last">&raquo;</a>
						</div>
					</nav>
				</div>
			</div>
		</div>
	</section>
</div>


<div class="modal fade modalalign" id="map-modal" tabindex="-1" role="dialog">
	<div class="modal-dialog" role="document">
		<div class="modal-content">
			<div class="modal-header">
				<button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
				<h4 class="modal-title">Location on Map</h4>
			</div>
			<div class="modal-body">
				<div id="map-area" style="width: 100%; height: 400px;"></div>
			</div>
		</div>
	</div>
</div>

<script type="text/template" id="job-template">
	<tr data-type="<%=jobtype%>">
		<td class="siteId"><%=siteId%></td>
		<td><%=location%></td>
		<td class="siteId"><%=siteId%></td>
		<td><%=state%></td>
		<td><%=PostalCode%></td>
		<td class="view-map" data-latitude="<%=latitude%>" data-longitude="<%=longitude%>" data-auditorlatitude="<%=auditorlatitude%>" data-auditorlongitude="<%=auditorlongitude%>">
			<i class="fa fa-location-arrow" aria-hidden="true"></i>
		</td>
		<td class="coloumn"><%=completeddate%></td>
		<td class="coloumn"><%=completeddate%></td>
	</tr>
</script>
<script type="text/template" id="pagination-template">
	
</script>
<script src="js/vendor/jquery-3.1.0.min.js"></script>
<script src="js/vendor/bootstrap.min.js"></script>
<script src="js/scripts.js"></script>
<script type="text/javascript" src="http://c3173.r73.cf2.rackcdn.com/Kumulos.min.js"></script>
<script src="js/vendor/underscore.min.js"></script>
<script src="js/vendor/sortElements.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.18.1/moment.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/stupidtable/1.0.3/stupidtable.min.js"></script>
<script type="text/javascript" src='http://maps.google.com/maps/api/js?sensor=false&key=AIzaSyC7lEhWOW6t8D5yWL0FwnL_Wa7QgyVNe7o&libraries=places'></script>
<script src="js/vendor/location.picker.min.js"></script>
<script src="http://www.jqueryscript.net/demo/Lightweight-Interactive-Pagination-Plugin-jQuery-jqPagination/js/jquery.jqpagination.js"></script>
<script src="js/api_scripts/jobs-assigned.js"></script>
<script src="js/api_scripts/user_data.js"></script>

</body>
</html>